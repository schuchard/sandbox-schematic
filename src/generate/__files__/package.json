{
    "name": "schematic-collection-name",
    "version": "1.0.0",
    "main": "src/schematics/index.js",
    "schematics": "./src/collection.json",
    "scripts": {
      "build:schematic": "tsc -p tsconfig.schematic.json",
      "build:schematic:watch": "tsc -p tsconfig.schematic.json -watch",
      "clean": "git checkout HEAD -- sandbox && git clean -fd sandbox",
      "clean:launch": "yarn clean && yarn launch",
      "clean:launch:all": "yarn build:schematic && yarn clean:launch",
      "launch": "cd sandbox && ng g schematic-collection-name:schematic-name",
      "test:ci": "yarn clean:launch && yarn test:sandbox && yarn clean",
      "test:sandbox": "cd sandbox && yarn lint && yarn test && yarn e2e && yarn build",
      "link:schematic": "yarn link && cd sandbox && yarn link schematic-collection-name",
      "setup:once": "yarn && yarn link:schematic && yarn"
    },
    "dependencies": {
      "@angular-devkit/core": "^0.7.2",
      "@angular-devkit/schematics": "^0.7.2"
    },
    "devDependencies": {
      "@angular-devkit/build-angular": "~0.7.2",
      "@angular/cli": "~6.1.2",
      "@angular/compiler-cli": "^6.1.1",
      "@angular/language-service": "^6.1.1",
      "@types/node": "^10.5.5",
      "codelyzer": "~4.4.2",
      "ts-node": "^7.0.0",
      "tslint": "~5.11.0",
      "typescript": "~3.0.1"
    }
  }
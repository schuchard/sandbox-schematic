{
  "name": "@schuchard/schematic-generator",
  "version": "1.0.0",
  "main": "src/schematics/index.js",
  "bin": "src/schematics/index.js",
  "schematics": "./src/collection.json",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:clean:launch": "yarn build && yarn clean:launch",
    "clean": "rm -rf sandbox && mkdir sandbox",
    "clean:launch": "yarn clean && yarn launch",
    "launch": "schematics --dry-run=false .:generate ",
    "test": "yarn build:clean:launch && yarn test:sandbox && yarn clean",
    "test:sandbox": "cd sandbox && yarn lint && yarn test && yarn e2e && yarn build",
    "link:schematic": "yarn link && cd sandbox && yarn link @user/schematic-collection"
  },
  "dependencies": {
    "@angular-devkit/core": "^7.0.4",
    "@angular-devkit/schematics": "^7.0.4",
    "@angular-devkit/schematics-cli": "^0.10.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.4",
    "@angular/cdk": "^7.0.2",
    "@angular/cli": "~7.0.4",
    "@angular/compiler-cli": "^7.0.2",
    "@angular/language-service": "^7.0.2",
    "@types/jasmine": "^2.8.9",
    "@types/node": "^10.12.2",
    "@schematics/angular": "^7.0.4",
    "codelyzer": "~4.5.0",
    "jasmine": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
